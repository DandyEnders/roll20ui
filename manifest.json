{
    "name": "roll20ui",
    "version": "1.0",
    "description": "Roll20 UI",
    "permissions": ["tabs", "storage"],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["jquery.js", "content.js"]
        }
    ],
    "browser_action":{
        "default_popup": "popup.html",
        "default_title": "roll20 ui"
    },

    "manifest_version": 2
    
}