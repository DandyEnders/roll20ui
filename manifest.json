{
    "name": "roll20ui",
    "version": "1.0",
    "description": "Roll20 UI",
    "permissions": ["tabs", "storage"],
    "content_scripts": [
        {
        "matches": ["https://app.roll20.net/*"],
        "js": ["jquery.js","BaseContent.js"]
        }
    ],
    "web_accessible_resources": [
        "content.js",
        "jquery.js"
	],
    "browser_action":{
        "default_popup": "popup.html",
        "default_title": "roll20 ui"
    },

    "manifest_version": 2
    
}